package com.panchuang.manager.model;

public class User {
	private int id = 0;
	private String name = null;
	private int age = 0;
	private String male = null;
	private float salary = 0f;
	public User() {
		super();
	}
	public User(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	
}
